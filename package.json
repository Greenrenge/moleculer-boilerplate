{
  "name": "backend-boilerplate",
  "version": "1.0.0",
  "description": "Backend Boilerplate",
  "main": "src/index.js",
  "exports": "./src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --loader esm-module-alias/loader --no-warnings --watch src ",
    "start:repl": "REPL=true npm run start",
    "start:prod": "cross-env NODE_ENV=production node --loader esm-module-alias/loader --no-warnings src/index.js",
    "lint": "eslint --ignore-path .eslintignore .",
    "lint:fix": "eslint --ignore-path .eslintignore . --fix",
    "test": "cross-env NODE_ENV=test node ./node_modules/mocha/bin/mocha",
    "test:watch": "cross-env NODE_ENV=test nodemon --watch test --watch src --exec npm run test",
    "migrate": "node --loader esm-module-alias/loader --no-warnings ./node_modules/migrate-mongo/bin/migrate-mongo.js",
    "migrate:create": "node --loader esm-module-alias/loader --no-warnings ./node_modules/migrate-mongo/bin/migrate-mongo.js create",
    "migrate:up": "node --loader esm-module-alias/loader --no-warnings ./node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "migrate:down": "node --loader esm-module-alias/loader --no-warnings ./node_modules/migrate-mongo/bin/migrate-mongo.js down",
    "migrate:status": "node --loader esm-module-alias/loader --no-warnings ./node_modules/migrate-mongo/bin/migrate-mongo.js status",
    "seed:simple": "cross-env LIGHTSHIP_RANDOM_PORT=true node --loader esm-module-alias/loader --no-warnings seeds/simple-seed.js",
    "seed:simple:prod": "cross-env NODE_ENV=production LIGHTSHIP_RANDOM_PORT=true node --loader esm-module-alias/loader --no-warnings seeds/simple-seed.js"
  },
  "keywords": [],
  "author": "GreenRenge",
  "license": "ISC",
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/mongoose": "^7.2.1",
    "@greenrenge/gracefully": "^1.1.0",
    "@meanie/mongoose-upsert-many": "^2.2.0",
    "@panva/jose": "^1.9.3",
    "agenda": "^5.0.0",
    "aws-sdk": "^2.1514.0",
    "axios": "^1.6.2",
    "base62": "^2.0.1",
    "bcrypt": "^5.1.1",
    "bluebird": "^3.7.2",
    "callsites": "^4.1.0",
    "cookie": "^0.6.0",
    "cron-parser": "^4.9.0",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^2.0.0",
    "dot-object": "^2.1.4",
    "esm-module-alias": "^2.1.0",
    "extract-files": "^11.0.0",
    "fastest-validator": "^1.17.0",
    "form-data": "^4.0.0",
    "got": "^14.1.0",
    "graphql": "^15.4.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.27.10",
    "jaeger-client": "^3.18.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "mailgun-js": "^0.22.0",
    "metascraper": "^5.43.4",
    "metascraper-description": "^5.43.4",
    "metascraper-image": "^5.43.4",
    "metascraper-title": "^5.43.4",
    "migrate-mongo": "^8.2.3",
    "moleculer": "^0.14.32",
    "moleculer-apollo-server": "^0.3.8",
    "moleculer-repl": "^0.7.4",
    "moleculer-web": "^0.10.7",
    "moment": "^2.30.1",
    "mongoose": "^8.0.2",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "mongoose-mpath": "^3.0.2",
    "nats": "^1.4.12",
    "onesignal-node": "^3.4.0",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "redlock": "^4.2.0",
    "sanitizer": "^0.1.3",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "factory-girl": "5.0.4",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^9.1.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0"
  },
  "overrides": {
    "extract-files": "^11.0.0"
  },
  "resolutions": {
    "extract-files": "^11.0.0"
  },
  "aliases": {
    "@": "src",
    "@test": "test",
    "@configs": "configs",
    "@pkg": "packages",
    "@utils": "src/utils"
  },
  "packageManager": "pnpm@8.14.1",
  "pnpm": {
    "patchedDependencies": {
      "extract-files@11.0.0": "patches/extract-files@11.0.0.patch",
      "factory-girl@5.0.4": "patches/factory-girl@5.0.4.patch"
    }
  }
}