{
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@user/*": [
        "src/user-service/*"
      ],
      "@org/*": [
        "src/organization-service/*"
      ],
      "@activity/*": [
        "src/activity-service/*"
      ],
      "@metadata/*": [
        "src/metadata-service/*"
      ],
      "@gateway/*": [
        "src/gateway-service/*"
      ],
      "@gateway-admin/*": [
        "src/gateway-admin-service/*"
