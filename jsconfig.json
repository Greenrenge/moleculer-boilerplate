      "@notification/*": [
        "src/notification-service/*"
      ],
      "@video/*": [
        "src/video-service/*"
      ],
      "@coaching/*": [
        "src/coaching-service/*"
      ],
      "@survey/*": [
        "src/survey-service/*"
      ],
      "@group/*": [
        "src/group-service/*"
      ],
      "@cms/*": [
        "src/cms/*"
      ],
      "@configs/*": [
